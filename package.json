{
  "name": "brewGet",
  "description": "brewGet is a web application that supports the non-monetary exchange of regionally-limited or otherwise difficult to acquire beer. Built with Node.js, Koa, and AngularJS.",
  "version": "0.1.1",
  "private": true,
  "author": {
    "name": "Michael J. Bondra",
    "email": "mjbondra@gmail.com",
    "url": "https://twitter.com/mjbondra"
  },
  "scripts": {
    "start": "NODE_ENV=development nodemon --harmony server",
    "test": "NODE_ENV=test PORT=3001 ./node_modules/.bin/mocha --harmony --reporter spec server/test/*.js",
    "install": "gulp"
  },
  "devDependencies": {
    "bower": "~1.2.8",
    "gulp": "~3.5.2",
    "gulp-bower": "~0.0.2",
    "gulp-browserify": "~0.4.2",
    "gulp-compass": "~1.1.2",
    "gulp-if": "~0.0.5",
    "gulp-minify-css": "~0.3.0",
    "gulp-plumber": "~0.5.6",
    "gulp-rename": "~1.1.0",
    "gulp-uglify": "~0.2.0",
    "mocha": "~1.17.1",
    "should": "~3.1.2",
    "supertest": "~0.9.0"
  },
  "dependencies": {
    "bluebird": "1.0.7",
    "co-body": "0.0.1",
    "co-busboy": "1.0.0",
    "co-fs": "1.1.1",
    "gm": "1.14.2",
    "gravatar": "1.0.6",
    "koa": "0.5.0",
    "koa-compress": "1.0.3",
    "koa-logger": "1.2.0",
    "koa-router": "3.0.2",
    "koa-session-mongoose": "0.2.2",
    "koa-session-store": "0.1.0",
    "koa-static": "1.4.3",
    "mime": "1.2.11",
    "mongoose": "3.8.3",
    "sanitizer": "0.1.1",
    "underscore": "1.6.0",
    "uid2": "0.0.3",
    "validator": "3.3.0"
  }
}
